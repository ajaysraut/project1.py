# Instance veriable with constructor
# student.py

class Student:
    def __init__(self,nm,m):
        self.name=nm
        self.marks=m

std1=Student('Ajay',89)
std2=Student('Vijay',70)

print(std1.__dict__)
# outside the class
std1.age=21
print(std1.__dict__)
print(std2.__dict__)
del std1.name
print(std1.__dict__)